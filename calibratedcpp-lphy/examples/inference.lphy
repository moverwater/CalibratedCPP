data {
  clade1 = ["leaf_45", "leaf_46", "leaf_47"];
  clade2 = ["leaf_45", "leaf_46", "leaf_47", "leaf_48", "leaf_49"];
  clade3 = ["leaf_87", "leaf_88"];
}
model {
    lambda ~ Uniform(lower=0.0, upper=10.0);
    mu ~ Uniform(lower=0.0, upper=10.0);

    calibrations ~ ConditionedMRCAPrior(calibrationTaxa=[clade1, clade2,clade3], upperBounds=[1.5,1.9,1.5], lowerBounds=[1.0,1.5,1.1]);
    tree ~ CalibratedCPP(calibrations=calibrations, lambda=lambda, mu=mu, n=10, rho=0.1, stemAge=5);
}