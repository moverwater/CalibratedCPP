<package name='calibratedcpp' version='0.0.1'>
    <depends on='BEAST.base' atleast='2.7.0'/>
    <depends on='BEAST.app' atleast='2.7.0'/>

    <service type="beast.base.core.BEASTInterface">
        <provider classname="calibratedcpp.CalibratedCoalescentPointProcess"/>
        <provider classname="calibratedcpp.model.CoalescentPointProcessModel"/>
        <provider classname="calibratedcpp.model.BirthDeathModel"/>
        <provider classname="calibratedcpp.model.BirthDeathSkylineModel"/>
        <provider classname="calibratedcpp.model.AgeDependentModel"/>
        <provider classname="calibrationprior.CalibrationPrior"/>
        <provider classname="calibrationprior.CalibrationCladePrior"/>
        <provider classname="calibration.CalibrationNode"/>
        <provider classname="calibration.CalibrationClade"/>
        <provider classname="calibration.CalibrationForest"/>
        <provider classname="calibrationprior.logger.MRCALogger"/>
    </service>
</package>
