<!--                        <distr spec="beast.base.inference.distribution.Uniform" lower="1.99" upper="2.01"/>-->
<!--                </calibrations>-->
            </distribution>
            <distribution id="birthRatePrior" spec="beast.base.inference.distribution.Prior" x="@birthRate">
                <distr spec="beast.base.inference.distribution.Uniform" lower="0.0" upper="10.0"/>
            </distribution>
            <distribution id="deathRatePrior" spec="beast.base.inference.distribution.Prior" x="@deathRate">
                <distr spec="beast.base.inference.distribution.Uniform" lower="0.0" upper="10.0"/>
            </distribution>
        </distribution>

        <operator id="birthRateScaler" spec="ScaleOperator" scaleFactor=".75" weight="1" parameter="@birthRate"/>
        <operator id="deathRateScaler" spec="ScaleOperator" scaleFactor=".75" weight="1" parameter="@deathRate"/>

        <logger fileName="./mcmc_output/$(filebase)_$(Index).log" id="tracelog" logEvery="1000" mode="autodetect">
            <log idref="birthDeath"/>
            <log idref="birthRate"/>
            <log idref="deathRate"/>
        </logger>

        <logger id="screenlog" logEvery="1000" mode="autodetect">
            <log arg="@birthDeath" id="ESS.0" spec="util.ESS"/>
            <log idref="birthDeath"/>
            <log idref="birthRate"/>
            <log idref="deathRate"/>
        </logger>
